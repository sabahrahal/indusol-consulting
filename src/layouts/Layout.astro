---
import '@fontsource/oswald';
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro' 

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Asesoría técnica y comercial para la industria alimenticia." />
		<meta name="author" content="Sabah Rahal">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link rel="icon" type="image/svg+xml" href="/favicon.ico" />
		<meta name="generator" content={Astro.generator} />
		<meta property="og:image" content="/logo-urbanprojectbcn.webp">
		<meta property="og:title" content="Indisul Consulting">
		<meta property="og:description" content="Asesoría técnica y comercial para la industria alimenticia."
		<meta property="og:image:width" content="200"> 
		<meta property="og:image:height" content="200">
		<meta property="og:url" content="indisulconsulting">
		<meta property="og:type" content="website">
		<title>{title}</title>
	</head>
	<body>
		<Navbar/>
		<slot />
		<Footer />
	</body>
</html>
